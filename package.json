{
  "name": "pizza-to-the-polls",
  "private": false,
  "version": "1.0.0",
  "description": "Pizza to the Polls",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pizza-to-the-polls/polls.pizza"
  },
  "scripts": {
    "dev": "stencil build --dev --watch --serve",
    "build": "npm run clean && stencil build --no-cache --prerender",
    "clean": "rimraf dist/www",
    "start": "npm run dev",
    "fix": "npm run prettier:fix && npm run lint:fix",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --force --project .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "generate": "stencil generate",
    "test": "stencil test --spec --e2e",
    "ci": "npm run prettier && npm run lint && npm run test"
  },
  "devDependencies": {
    "@stencil/core": "^1.17.3",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@types/jest": "25.2.3",
    "@types/node": "^14.0.27",
    "@types/puppeteer": "2.0.1",
    "dotenv": "^8.2.0",
    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "prettier": "^2.1.2",
    "puppeteer": "2.1.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {}
}
